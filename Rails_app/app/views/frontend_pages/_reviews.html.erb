<% @rating.each do |rating| %>
  <div class="thumbnail comment row">
      <div class="col-xs-6  col-md-2 col-lg-2 rating ">
<%# Stars %>
        <% (1..rating.grade).each do |full| %>
          <span class="glyphicon glyphicon-star" aria-hidden="true"></span>
        <% end %>
        <% (1..(5-rating.grade)).each do |empty| %>
          <span class="glyphicon glyphicon-star-empty" aria-hidden="true"></span>
        <% end %>
<%# end Stars %>
      </div>
      <div class="col-xs-6  col-md-3 col-lg-4 col-md-push-6 created-at text-right"><%= rating.created_at %></div>
      <div class="col-sm-12 col-md-6 col-md-pull-3 username">By: <%= rating.customer_id %></div>
      <div class="review col-xs-12"><%= rating.review %></div>
    </div>
<% end %>